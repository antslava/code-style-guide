<?xml version="1.0" encoding="UTF-8"?>
<FindBugsFilter>
    <!-- Ignore Android generated classes -->
    <Match>
        <Class name="~.*\.R\$.*" />
    </Match>
    <Match>
        <Class name="~.*\.R2\$.*" />
    </Match>
    <Match>
        <Class name="~.*\.Manifest\$.*" />
    </Match>

    <Match>
        <Class name="~.*\.MediaCollection" />
    </Match>

    <Match>
        <Class name="~.*\.MediaItem" />
    </Match>

    <!-- Ignore unsued fields for classes that ends with 'Request' -->
    <Match>
        <Class name="~.*Request" />
        <Bug code="UrF" />
    </Match>
    <!-- Ignore JUnit test classes - other than JUnit issues -->
    <Match>
        <Class name="~.*\.*Test" />
        <Not>
            <Bug code="IJU" />
        </Not>
    </Match>
</FindBugsFilter>
